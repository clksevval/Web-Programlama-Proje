

<h1 style="color:black; text-align:center;margin:50px">Randevuların Listesi</h1>


<table class="table table-striped table-hover">
    <tr>
        <td>Randevu Id</td>
        <td>Öğretim Üyesi Id</td>
        <td>Asistan Id</td>
        <td>Randevu Tarihi</td>
    </tr>
    @foreach (var item in ViewBag.RandevuListe as List<Randevu>)
    {

        <tr>
            <td>@item.RandevuId</td>
            <td>@item.HocaId</td>
            <td>@item.AsistanId</td>
            <td>@item.Randevu_Tarihi</td>
            <td>
                <a class="btn btn-info" asp-action="Update_Randevu" asp-route-id="@item.RandevuId">Detay</a>
            </td>

        </tr>
    }

</table>



